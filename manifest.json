{
    "manifest_version": 2,

    "name": "Instagram Black",
    "description": "Autosave instagram stories anonymously, or surf on instagram as ghost. And you can get notifications with Telegram Bot.",
    "version": "1.62",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
    },
    "permissions": [
        "https://api.telegram.org/bot*",
        "https://*.instagram.com/*",
        "webRequest",
        "webRequestBlocking",
        "storage",
        "downloads"
    ],
    "background": {
        "scripts": ["background.js"]
    }
}
